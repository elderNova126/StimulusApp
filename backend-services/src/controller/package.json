{
  "name": "controller",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "install-dev": "yarn install",
    "proto-update": "rm -r node_modules/controller-proto/ && yarn install --check-files",
    "start": "nest start",
    "start.dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npx eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "test": "yarn spell && yarn lint && yarn test:cov",
    "typeorm:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config dist/database/config/typeorm-global.config.js",
    "typeorm:migrate": "ts-node node_modules/typeorm/cli.js migration:run --config dist/database/config/typeorm-global.config.js",
    "typeorm:migrate:tenant": "ts-node node_modules/typeorm/cli.js migration:run --config dist/database/config/typeorm-tenant.config.js",
    "typeorm:create:global": "ts-node node_modules/typeorm/cli.js migration:create -d src/database/migrations/global",
    "typeorm:create:tenant": "ts-node node_modules/typeorm/cli.js migration:create -d src/database/migrations/tenant",
    "typeorm:revert:global": "ts-node node_modules/typeorm/cli.js migration:revert --config dist/database/config/typeorm-global.config.js",
    "typeorm:revert:tenant": "ts-node node_modules/typeorm/cli.js migration:revert --config dist/database/config/typeorm-tenant.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "spell": "npx cspell '**/*.{js,jsx,ts,tsx,json,yml,yaml}'"
  },
  "dependencies": {
    "@azure/identity": "1.5.2",
    "@azure/keyvault-secrets": "4.9.0",
    "@azure/msal-node": "^3.2.2",
    "@azure/search-documents": "11.3.3",
    "@grpc/grpc-js": "1.12.6",
    "@grpc/proto-loader": "0.5.6",
    "@microsoft/microsoft-graph-client": "2.2.1",
    "@nestjs/bull": "0.3.1",
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "0.2.4",
    "@nestjs/core": "10.4.15",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/microservices": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/terminus": "8.1.1",
    "@nestjs/typeorm": "9.0.1",
    "@sendgrid/mail": "7.7.0",
    "@types/bcrypt": "5.0.2",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "14.18.63",
    "adal-node": "0.2.4",
    "axios": "0.19.2",
    "azure-search-client": "3.1.5",
    "bcrypt": "5.1.1",
    "bull": "3.29.3",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "controller-proto": "file:../proto/controller",
    "crypto": "1.0.1",
    "dd-trace": "3.58.0",
    "dotenv": "16.4.7",
    "ein-validator": "1.0.1",
    "eslint-plugin-jsdoc": "32.3.4",
    "guid": "0.0.12",
    "i18n-iso-countries": "7.13.0",
    "ioredis": "^5.5.0",
    "iso-3166-1-alpha-2": "1.0.2",
    "iso-3166-2": "1.0.0",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "kafkajs": "1.16.0",
    "moment": "2.30.1",
    "mssql": "^11.0.1",
    "nestjs-ddtrace": "3.0.4",
    "node-cache": "5.1.0",
    "ramda": "0.28.0",
    "redis": "4.7.0",
    "reflect-metadata": "0.1.14",
    "rimraf": "3.0.0",
    "rxjs": "6.6.7",
    "supertest": "7.0.0",
    "tslib": "2.8.1",
    "typeorm": "0.2.45",
    "winston": "3.17.0",
    "winston-console-formatter": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.26",
    "@nestjs/axios": "3.1.3",
    "@nestjs/cli": "6.14.2",
    "@nestjs/testing": "10.4.15",
    "@types/bull": "3.15.9",
    "@types/cache-manager": "4.0.6",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cspell": "6.31.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "29.7.0",
    "prettier": "3.5.1",
    "ts-jest": "29.2.5",
    "ts-loader": "6.2.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.15.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
